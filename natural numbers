import java.util.Scanner;
public class Naturalnumbers
{
    public static void main(String[]arg)
    {
        Scanner input=new Scanner(System.in);
        System.out.println("enter:");
        int sum=input.nextInt();
        for(int j=1;j<=10;j++)
        {
         sum=sum+j;
        }
        System.out.println(sum);
     }
}
